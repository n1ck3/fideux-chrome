{
    "name": "teuxdeux for chrome",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Simple Google Chrome extension to show today's teuxdeuxs",
    "browser_action": {
        "default_icon": "static/img/icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "static/img/icon_16x16.png",
        "48": "static/img/icon_48x48.png",
        "128": "static/img/icon_128x128.png"
    },
    "options_page": "options.html",
    "permissions": [
        "https://www.teuxdeux.com/api/*"
    ],
    "offline_enabled": false
}
